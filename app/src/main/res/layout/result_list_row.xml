<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <RelativeLayout
        android:id="@+id/datelLayout"
        android:layout_width="65sp"
        android:layout_height="65sp"
        android:layout_gravity="center"
        android:layout_marginLeft="5dp">

        <RelativeLayout
            android:id="@+id/bodyLayout"
            android:layout_width="65sp"
            android:layout_height="65sp"
            android:background="@drawable/circle_calendar"
            android:orientation="vertical"
            android:padding="5dp">

            <com.trendinganalysis.conetrading.RobotoTextViewBold
                android:id="@+id/dayText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="8sp"
                android:text="DD"
                android:textColor="@color/white"
                android:textSize="35sp" />

            <com.trendinganalysis.conetrading.RobotoTextViewBold
                android:id="@+id/yearText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="YYYY"
                android:textColor="@color/white"
                android:textSize="10sp" />

        </RelativeLayout>

        <FrameLayout
            android:layout_width="20sp"
            android:layout_height="20sp"
            android:layout_alignRight="@+id/bodyLayout"
            android:background="@drawable/circle_calendar_month">

            <com.trendinganalysis.conetrading.RobotoTextViewBold
                android:id="@+id/monthText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="MMM"
                android:textColor="@color/white"
                android:textSize="8sp" />


        </FrameLayout>


    </RelativeLayout>


    <!--<ImageView-->
    <!--android:id="@+id/imageView1"-->
    <!--android:layout_width="@dimen/result_list_iconSize"-->
    <!--android:layout_height="@dimen/result_list_iconSize"-->
    <!--android:layout_gravity="center"-->
    <!--android:layout_marginLeft="5dp"-->
    <!--android:src="@drawable/ic_date" />-->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="10dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <com.trendinganalysis.conetrading.RobotoTextViewBold
                android:id="@+id/makerData"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:text="#maker"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="@dimen/medium_text"
                android:textStyle="normal" />

            <com.trendinganalysis.conetrading.RobotoTextViewBold
                android:id="@+id/seriesData"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/medium_text"
                android:textStyle="normal" />

            <com.trendinganalysis.conetrading.RobotoTextViewBold
                android:id="@+id/typeData"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/medium_text"
                android:textStyle="normal" />


        </LinearLayout>


        <com.trendinganalysis.conetrading.RobotoTextViewRegular
            android:id="@+id/txtDesc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dp"
            android:text="@string/item_description"
            android:textColor="@color/desc_fontColor"
            android:textSize="@dimen/micro_text"
            android:textStyle="normal" />


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp">

                    <com.trendinganalysis.conetrading.RobotoTextViewRegular
                        android:id="@+id/typeLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Model: "
                        android:textColor="@color/desc_fontColor"
                        android:textSize="@dimen/micro_text"
                        android:textStyle="normal" />

                    <com.trendinganalysis.conetrading.RobotoTextViewRegular
                        android:id="@+id/modelData"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="#model"
                        android:textColor="@color/desc_fontColor"
                        android:textSize="@dimen/micro_text"
                        android:textStyle="normal" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp">

                    <com.trendinganalysis.conetrading.RobotoTextViewRegular
                        android:id="@+id/makerLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Engine: "
                        android:textColor="@color/desc_fontColor"
                        android:textSize="@dimen/micro_text"
                        android:textStyle="normal" />

                    <com.trendinganalysis.conetrading.RobotoTextViewRegular
                        android:id="@+id/txtEngine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="#engine"
                        android:textColor="@color/desc_fontColor"
                        android:textSize="@dimen/micro_text" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp">

                    <com.trendinganalysis.conetrading.RobotoButtonRegular
                        android:layout_width="@dimen/currency_width"
                        android:layout_height="20dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/php_currency_style"
                        android:text="PHP"
                        android:textSize="@dimen/micro_text" />

                    <com.trendinganalysis.conetrading.RobotoTextViewRegular
                        android:id="@+id/pesoPriceData"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="2dp"
                        android:text="123,123,123"
                        android:textColor="@color/desc_fontColor"
                        android:textSize="@dimen/micro_text"
                        android:textStyle="normal" />

                </LinearLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/result_row_text_padding_left">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="2dp">

                        <com.trendinganalysis.conetrading.RobotoTextViewRegular
                            android:id="@+id/sizeLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Weight: "
                            android:textColor="@color/desc_fontColor"
                            android:textSize="@dimen/micro_text"
                            android:textStyle="normal" />

                        <com.trendinganalysis.conetrading.RobotoTextViewRegular
                            android:id="@+id/sizeData"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="#weight"
                            android:textColor="@color/desc_fontColor"
                            android:textSize="@dimen/micro_text"
                            android:textStyle="normal" />
                    </LinearLayout>


                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="2dp">

                    <com.trendinganalysis.conetrading.RobotoTextViewRegular
                        android:id="@+id/categoryLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Source: "
                        android:textColor="@color/desc_fontColor"
                        android:textSize="@dimen/micro_text"
                        android:textStyle="normal" />

                    <com.trendinganalysis.conetrading.RobotoTextViewRegular
                        android:id="@+id/categoryData"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="#Source"
                        android:textColor="@color/desc_fontColor"
                        android:textSize="@dimen/micro_text"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">



                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <com.trendinganalysis.conetrading.RobotoButtonRegular
                            android:layout_width="@dimen/currency_width"
                            android:layout_height="20dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/jpy_currency_style"
                            android:text="JPY"
                            android:textSize="@dimen/micro_text" />

                        <com.trendinganalysis.conetrading.RobotoTextViewRegular
                            android:id="@+id/yenPriceData"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="2dp"
                            android:text="123,123,123"
                            android:textColor="@color/desc_fontColor"
                            android:textSize="@dimen/micro_text"
                            android:textStyle="normal" />


                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="@dimen/result_row_text_padding_left">


            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>